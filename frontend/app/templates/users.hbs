<div class="users_wrapper">
    <div class="group_create_bar">
        {{input value=newNameGroup placeholder="Name Group"}}
        <button {{action "createGroup"}}>Create group</button>
    </div>
    <div class="users_list">
        <ul>
            {{#each users as |user|}}
            {{user-elem firstName=user.firstName lastName=user.lastName id=user.id action="addUser" }}
            {{/each}}
        </ul>
    </div>
    {{pagination-bar}}
    <div class="cat_fact">
        {{catFact}}
    </div>
</div>
{{outlet}}